<?xml version="1.0" encoding="UTF-8"?>
<CustomMetadata xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <label>Credit Cards</label>
    <protected>false</protected>
    <values>
        <field>Block_Message__c</field>
        <value xsi:type="xsd:string">Blocked! Credit Card # in the content. Please remove. If still required add &quot;(!cc)&quot; immediately after the # or &quot;(!moderation)&quot; as first letters which will send this content for review &amp; approval.</value>
    </values>
    <values>
        <field>Block_REGEX_Patterns__c</field>
        <value xsi:type="xsd:string">^[^0-9]*4[0-9]{15}[^0-9]*$
^[^0-9]*(?:5[1-5][0-9]{2}|222[1-9]|22[3-9][0-9]|2[3-6][0-9]{2}|27[01][0-9]|2720)[0-9]{12}[^0-9]*$
^[^0-9]*3[47][0-9]{13}[^0-9]*$</value>
    </values>
    <values>
        <field>Block_Words__c</field>
        <value xsi:nil="true"/>
    </values>
    <values>
        <field>Escape_Patterns__c</field>
        <value xsi:type="xsd:string">^\(\!moderation\)[\w\W]*$
^[^0-9]*4[0-9]{15}\(\!cc\)[^0-9]*$
^[^0-9]*(?:5[1-5][0-9]{2}|222[1-9]|22[3-9][0-9]|2[3-6][0-9]{2}|27[01][0-9]|2720)[0-9]{12}\(\!cc\)[^0-9]*$
^[^0-9]*3[47][0-9]{13}\(\!cc\)[^0-9]*$</value>
    </values>
    <values>
        <field>IsActive__c</field>
        <value xsi:type="xsd:boolean">true</value>
    </values>
</CustomMetadata>
